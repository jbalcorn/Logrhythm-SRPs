<?xml version="1.0" encoding="utf-8"?>
<remv1:Remediation-Plugin xmlns:remv1="RemediationVersion1.xsd"
 Name="telegram notification SRP">
 <remv1:Action Name="Send notification"
 Command="C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe">
 <remv1:ConstantParameter Name="Script" Switch="-file send_telegram_msg.ps1"
 Order="1" />
 <remv1:StringParameter Name="channel ID" Switch="" Order="2" />
 <remv1:StringParameter Name="API Key" Switch="" Order="3" />
 <remv1:StringParameter Name="Message" Switch="" Order="4" />
 <remv1:StringParameter Name="param 1" Switch="" Order="5" />
 <remv1:StringParameter Name="param 2" Switch="" Order="6" />
 <remv1:StringParameter Name="param 3" Switch="" Order="7" />
 <remv1:StringParameter Name="param 4" Switch="" Order="8" />
 <remv1:StringParameter Name="Proxy URI" Switch="" Order="9" />
 <remv1:StringParameter Name="Use Service creds" Switch="" Order="10" />
</remv1:Action>


</remv1:Remediation-Plugin>